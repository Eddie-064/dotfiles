alias vim="nvim"
alias dot="cd ~/.local/share/chezmoi/"  # same as `chezmoi cd`
alias repo="cd ~/repo"
alias clr="clear"
alias lg="lazygit"
alias myip="curl ipinfo.io"
alias ga="git add"
alias gs="git status"
alias gd="git diff"

export PATH="$HOME/Library/Android/sdk/platform-tools:$PATH"
export PATH="$HOME/Library/Android/sdk/ndk-bundle:$PATH"
